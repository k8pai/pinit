{
	"manifest_version": 3,
	"version": "1.0",
	"name": "Pinit",
	"action": {
		"default_popup": "index.html",
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["index.js"]
		}
	],
	"permissions": [
		"tabs",
		"http://localhost:3000/*",
		"https://linkwrap.k8pai.dev"
	]
}
